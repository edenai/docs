                "type": "object",
                "properties": {
                    "<parameter_name>": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "<parameter_name>"
                ]
            },
            "NestedBadRequest": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string"
                    },
                    "message": {
                        "$ref": "#/components/schemas/FieldError"
                    }
                },
                "required": [
                    "message",
                    "type"
                ]
