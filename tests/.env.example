# Eden AI test configuration
# Copy to .env and fill in values:  cp .env.example .env

# Sandbox token — used for most snippet tests (AI feature calls return mock responses)
EDEN_AI_SANDBOX_API_TOKEN=your_sandbox_token_here

# Production token — needed for v2 admin endpoint tests (cost management, token management)
# Optional: tests that need this will be skipped if not set
EDEN_AI_PRODUCTION_API_TOKEN=your_api_token_here

EDEN_AI_BASE_URL=https://api.edenai.run
